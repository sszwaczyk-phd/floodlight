		Module Link : Node23_Link_InterfaceID_1
			Set Node23_Link_InterfaceID_1.guitag_remotenode = yes


		Bind Node23_Interface_InterfaceID_1 Node23_MAC8023_InterfaceID_1
		Bind Node23_MAC8023_InterfaceID_1 Node23_PHY_InterfaceID_1
		Bind Node23_PHY_InterfaceID_1 Node23_Link_InterfaceID_1
	EndDefine

EndCreate


