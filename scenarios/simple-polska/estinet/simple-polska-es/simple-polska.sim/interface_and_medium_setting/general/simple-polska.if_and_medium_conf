			Set Node12_MAC8023_InterfaceID_5.UniInOutLogFile = simple-polska.mac8023_n12_i5_uni_inout.log
			Set Node12_MAC8023_InterfaceID_5.BroInLogFile = simple-polska.mac8023_n12_i5_bro_in.log
			Set Node12_MAC8023_InterfaceID_5.BroOutLogFile = simple-polska.mac8023_n12_i5_bro_out.log
			Set Node12_MAC8023_InterfaceID_5.BroInOutLogFile = simple-polska.mac8023_n12_i5_bro_inout.log
			Set Node12_MAC8023_InterfaceID_5.CollLogFile = simple-polska.mac8023_n12_i5_collision.log
			Set Node12_MAC8023_InterfaceID_5.DropLogFile = simple-polska.mac8023_n12_i5_drop.log
			Set Node12_MAC8023_InterfaceID_5.InThrputLogFile = simple-polska.mac8023_n12_i5_in_thrput.log
			Set Node12_MAC8023_InterfaceID_5.OutThrputLogFile = simple-polska.mac8023_n12_i5_out_thrput.log
			Set Node12_MAC8023_InterfaceID_5.InOutThrputLogFile = simple-polska.mac8023_n12_i5_inout_thrput.log
			Set Node12_MAC8023_InterfaceID_5.LinktoHub = 0

		Module PHY : Node12_PHY_InterfaceID_5
			Set Node12_PHY_InterfaceID_5.datarate = 100_Mbps
			Set Node12_PHY_InterfaceID_5.BER = 0.0
			Set Node12_PHY_InterfaceID_5.PropDelay = 1.0
			Set Node12_PHY_InterfaceID_5.linkfail = off
			Set Node12_PHY_InterfaceID_5.linkfailFileName = simple-polska.phy8023_n12_i5.link_failure_duration

		Module Link : Node12_Link_InterfaceID_5
			Set Node12_Link_InterfaceID_5.guitag_remotenode = yes


		Bind Node12_Interface_InterfaceID_5 Node12_MAC8023_InterfaceID_5
		Bind Node12_MAC8023_InterfaceID_5 Node12_PHY_InterfaceID_5
		Bind Node12_PHY_InterfaceID_5 Node12_Link_InterfaceID_5
	EndDefine

		Bind Node12_Interface_0 Node12_OFLO_0
		Bind Node12_OFLO_0 Node12_OFSwitchV13_0
EndCreate


