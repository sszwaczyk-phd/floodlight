			Set Node7_MAC8023_InterfaceID_5.InOutThrputLogFile = simple-polska.mac8023_n7_i5_inout_thrput.log
			Set Node7_MAC8023_InterfaceID_5.LinktoHub = 0

		Module PHY : Node7_PHY_InterfaceID_5
