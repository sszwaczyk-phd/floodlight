			Set Node7_MAC8023_InterfaceID_5.UniOutLogFile = simple-polska.mac8023_n7_i5_uni_out.log
			Set Node7_MAC8023_InterfaceID_5.UniInOutLogFile = simple-polska.mac8023_n7_i5_uni_inout.log
			Set Node7_MAC8023_InterfaceID_5.BroInLogFile = simple-polska.mac8023_n7_i5_bro_in.log
			Set Node7_MAC8023_InterfaceID_5.BroOutLogFile = simple-polska.mac8023_n7_i5_bro_out.log
