Create Node 12 as OF_SW_V131 with name = OF_SW_V13112
	Module Interface : Node12_Interface_0
		Set Node12_Interface_0.tunnel_type = tap
		Set Node12_Interface_0.nat_id = 0
		Set Node12_Interface_0.if_type = 8023
