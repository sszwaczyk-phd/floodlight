			Set Node3_MAC8023_InterfaceID_5.BroOutLogFile = simple-polska.mac8023_n3_i5_bro_out.log
			Set Node3_MAC8023_InterfaceID_5.BroInOutLogFile = simple-polska.mac8023_n3_i5_bro_inout.log
			Set Node3_MAC8023_InterfaceID_5.CollLogFile = simple-polska.mac8023_n3_i5_collision.log
			Set Node3_MAC8023_InterfaceID_5.DropLogFile = simple-polska.mac8023_n3_i5_drop.log
			Set Node3_MAC8023_InterfaceID_5.InThrputLogFile = simple-polska.mac8023_n3_i5_in_thrput.log
			Set Node3_MAC8023_InterfaceID_5.OutThrputLogFile = simple-polska.mac8023_n3_i5_out_thrput.log
			Set Node3_MAC8023_InterfaceID_5.InOutThrputLogFile = simple-polska.mac8023_n3_i5_inout_thrput.log
			Set Node3_MAC8023_InterfaceID_5.LinktoHub = 0

		Module PHY : Node3_PHY_InterfaceID_5
			Set Node3_PHY_InterfaceID_5.datarate = 100_Mbps
			Set Node3_PHY_InterfaceID_5.BER = 0.0
			Set Node3_PHY_InterfaceID_5.PropDelay = 1.0
			Set Node3_PHY_InterfaceID_5.linkfail = off
			Set Node3_PHY_InterfaceID_5.linkfailFileName = simple-polska.phy8023_n3_i5.link_failure_duration

		Module Link : Node3_Link_InterfaceID_5
			Set Node3_Link_InterfaceID_5.guitag_remotenode = yes


		Bind Node3_Interface_InterfaceID_5 Node3_MAC8023_InterfaceID_5
		Bind Node3_MAC8023_InterfaceID_5 Node3_PHY_InterfaceID_5
		Bind Node3_PHY_InterfaceID_5 Node3_Link_InterfaceID_5
	EndDefine

		Bind Node3_Interface_0 Node3_OFLO_0
		Bind Node3_OFLO_0 Node3_OFSwitchV13_0
EndCreate


