Create Node 7 as OF_SW_V131 with name = OF_SW_V1317
	Module Interface : Node7_Interface_0
		Set Node7_Interface_0.tunnel_type = tap
		Set Node7_Interface_0.nat_id = 0
		Set Node7_Interface_0.if_type = 8023
		Set Node7_Interface_0.if_name = eth4
		Set Node7_Interface_0.max_qlen = 50
		Set Node7_Interface_0.log_qlen = off
		Set Node7_Interface_0.log_option = FullLog
		Set Node7_Interface_0.samplerate = 1
		Set Node7_Interface_0.logFileName = simple-polska.interface_n7_i0_qlen.log
		Set Node7_Interface_0.EnableClassify = on
		Set Node7_Interface_0.ClassifyFileName = simple-polska.n7_i0.flow_rule
	Module OFLO : Node7_OFLO_0
		Set Node7_OFLO_0.mac = 00:01:00:00:00:5b
	Module OFSwitchV13 : Node7_OFSwitchV13_0
		Set Node7_OFSwitchV13_0.guitag_addInterface = yes
		Set Node7_OFSwitchV13_0.guitag_addInterface = yes
		Set Node7_OFSwitchV13_0.flow_table_amount = 10
		Set Node7_OFSwitchV13_0.flow_table_entry_amount = 256
		Set Node7_OFSwitchV13_0.group_table_entry_amount = 64
		Set Node7_OFSwitchV13_0.meter_table_entry_amount = 64
		Set Node7_OFSwitchV13_0.table_miss_buffer_size = 1000
		Set Node7_OFSwitchV13_0.table_miss_action = Controller
		Set Node7_OFSwitchV13_0.controller_ip = 1.0.1.1
		Set Node7_OFSwitchV13_0.controller_tcp_listen_port = 6653
		Set Node7_OFSwitchV13_0.port_to_controller = 5
		Set Node7_OFSwitchV13_0.controller_id = master_ctl
		Set Node7_OFSwitchV13_0.enable_2nd_controller = off
		Set Node7_OFSwitchV13_0.controller_ip_2nd = 0.0.0.0
		Set Node7_OFSwitchV13_0.controller_tcp_listen_port_2nd = 6633
		Set Node7_OFSwitchV13_0.port_to_controller_2nd = 5
		Set Node7_OFSwitchV13_0.controller_id_2nd = master_ctl_2
		Set Node7_OFSwitchV13_0.enable_3rd_controller = off
