			Set Node9_PHY_InterfaceID_6.linkfail = off
			Set Node9_PHY_InterfaceID_6.linkfailFileName = simple-polska.phy8023_n9_i6.link_failure_duration

		Module Link : Node9_Link_InterfaceID_6
			Set Node9_Link_InterfaceID_6.guitag_remotenode = yes


		Bind Node9_Interface_InterfaceID_6 Node9_MAC8023_InterfaceID_6
		Bind Node9_MAC8023_InterfaceID_6 Node9_PHY_InterfaceID_6
		Bind Node9_PHY_InterfaceID_6 Node9_Link_InterfaceID_6
	EndDefine

		Bind Node9_Interface_0 Node9_OFLO_0
		Bind Node9_OFLO_0 Node9_OFSwitchV13_0
EndCreate


