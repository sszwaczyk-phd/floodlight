			Set Node17_MAC8023_InterfaceID_1.BroOutLogFile = simple-polska.mac8023_n17_i1_bro_out.log
			Set Node17_MAC8023_InterfaceID_1.BroInOutLogFile = simple-polska.mac8023_n17_i1_bro_inout.log
