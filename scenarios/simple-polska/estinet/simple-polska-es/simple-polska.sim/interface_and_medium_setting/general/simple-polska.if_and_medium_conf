		Set Node7_OFSwitchV13_0.controller_tcp_listen_port_3rd = 6633
		Set Node7_OFSwitchV13_0.port_to_controller_3rd = 5
		Set Node7_OFSwitchV13_0.controller_id_3rd = master_ctl_3
