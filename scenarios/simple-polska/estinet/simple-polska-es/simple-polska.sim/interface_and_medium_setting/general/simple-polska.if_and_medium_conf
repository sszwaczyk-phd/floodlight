		Module Link : Node7_Link_InterfaceID_5
			Set Node7_Link_InterfaceID_5.guitag_remotenode = yes


		Bind Node7_Interface_InterfaceID_5 Node7_MAC8023_InterfaceID_5
		Bind Node7_MAC8023_InterfaceID_5 Node7_PHY_InterfaceID_5
		Bind Node7_PHY_InterfaceID_5 Node7_Link_InterfaceID_5
	EndDefine

		Bind Node7_Interface_0 Node7_OFLO_0
		Bind Node7_OFLO_0 Node7_OFSwitchV13_0
EndCreate


