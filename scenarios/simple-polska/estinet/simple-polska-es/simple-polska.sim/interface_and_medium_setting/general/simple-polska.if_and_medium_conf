Create Node 3 as OF_SW_V131 with name = OF_SW_V1313
	Module Interface : Node3_Interface_0
		Set Node3_Interface_0.tunnel_type = tap
		Set Node3_Interface_0.nat_id = 0
		Set Node3_Interface_0.if_type = 8023
		Set Node3_Interface_0.if_name = eth4
		Set Node3_Interface_0.max_qlen = 50
		Set Node3_Interface_0.log_qlen = off
		Set Node3_Interface_0.log_option = FullLog
		Set Node3_Interface_0.samplerate = 1
		Set Node3_Interface_0.logFileName = simple-polska.interface_n3_i0_qlen.log
		Set Node3_Interface_0.EnableClassify = on
		Set Node3_Interface_0.ClassifyFileName = simple-polska.n3_i0.flow_rule
	Module OFLO : Node3_OFLO_0
		Set Node3_OFLO_0.mac = 00:01:00:00:00:5d
	Module OFSwitchV13 : Node3_OFSwitchV13_0
		Set Node3_OFSwitchV13_0.guitag_addInterface = yes
		Set Node3_OFSwitchV13_0.guitag_addInterface = yes
		Set Node3_OFSwitchV13_0.flow_table_amount = 10
		Set Node3_OFSwitchV13_0.flow_table_entry_amount = 256
		Set Node3_OFSwitchV13_0.group_table_entry_amount = 64
		Set Node3_OFSwitchV13_0.meter_table_entry_amount = 64
		Set Node3_OFSwitchV13_0.table_miss_buffer_size = 1000
		Set Node3_OFSwitchV13_0.table_miss_action = Controller
		Set Node3_OFSwitchV13_0.controller_ip = 1.0.1.1
		Set Node3_OFSwitchV13_0.controller_tcp_listen_port = 6653
		Set Node3_OFSwitchV13_0.port_to_controller = 5
		Set Node3_OFSwitchV13_0.controller_id = master_ctl
		Set Node3_OFSwitchV13_0.enable_2nd_controller = off
		Set Node3_OFSwitchV13_0.controller_ip_2nd = 0.0.0.0
		Set Node3_OFSwitchV13_0.controller_tcp_listen_port_2nd = 6633
		Set Node3_OFSwitchV13_0.port_to_controller_2nd = 5
		Set Node3_OFSwitchV13_0.controller_id_2nd = master_ctl_2
		Set Node3_OFSwitchV13_0.enable_3rd_controller = off
		Set Node3_OFSwitchV13_0.controller_ip_3rd = 0.0.0.0
		Set Node3_OFSwitchV13_0.controller_tcp_listen_port_3rd = 6633
		Set Node3_OFSwitchV13_0.port_to_controller_3rd = 5
		Set Node3_OFSwitchV13_0.controller_id_3rd = master_ctl_3
		Set Node3_OFSwitchV13_0.enable_4th_controller = off
		Set Node3_OFSwitchV13_0.controller_ip_4th = 0.0.0.0
		Set Node3_OFSwitchV13_0.controller_tcp_listen_port_4th = 6633
		Set Node3_OFSwitchV13_0.port_to_controller_4th = 5
		Set Node3_OFSwitchV13_0.controller_id_4th = master_ctl_4
		Set Node3_OFSwitchV13_0.enable_1st_lag = off
		Set Node3_OFSwitchV13_0.lag_port_no_1st = 1000
		Set Node3_OFSwitchV13_0.lag_aggr_port_1st = 1,2
		Set Node3_OFSwitchV13_0.lag_algorithm_1st = ROUND_ROBIN
		Set Node3_OFSwitchV13_0.lag_algo_param_1st = 1
		Set Node3_OFSwitchV13_0.enable_2nd_lag = off
		Set Node3_OFSwitchV13_0.lag_port_no_2nd = 1001
		Set Node3_OFSwitchV13_0.lag_aggr_port_2nd = 3,4
		Set Node3_OFSwitchV13_0.lag_algorithm_2nd = ROUND_ROBIN
		Set Node3_OFSwitchV13_0.lag_algo_param_2nd = 1
		Set Node3_OFSwitchV13_0.enable_3rd_lag = off
		Set Node3_OFSwitchV13_0.lag_port_no_3rd = 0
		Set Node3_OFSwitchV13_0.lag_aggr_port_3rd = 5,6,7,8...
		Set Node3_OFSwitchV13_0.lag_algorithm_3rd = ROUND_ROBIN
		Set Node3_OFSwitchV13_0.lag_algo_param_3rd = NULL
		Set Node3_OFSwitchV13_0.enable_1st_tunnel = off
		Set Node3_OFSwitchV13_0.tunnel_port_no_1st = 1002
		Set Node3_OFSwitchV13_0.tunnel_id_1st = 1
		Set Node3_OFSwitchV13_0.tunnel_protocol_1st = 4
		Set Node3_OFSwitchV13_0.tunnel_src_ip_1st = 192.168.0.3
		Set Node3_OFSwitchV13_0.tunnel_dst_ip_1st = 192.168.0.4
		Set Node3_OFSwitchV13_0.tunnel_out_port_1st = 3
		Set Node3_OFSwitchV13_0.enable_2nd_tunnel = off
		Set Node3_OFSwitchV13_0.tunnel_port_no_2nd = 1003
		Set Node3_OFSwitchV13_0.tunnel_id_2nd = 1
		Set Node3_OFSwitchV13_0.tunnel_protocol_2nd = 4
		Set Node3_OFSwitchV13_0.tunnel_src_ip_2nd = 192.168.0.5
		Set Node3_OFSwitchV13_0.tunnel_dst_ip_2nd = 192.168.0.6
		Set Node3_OFSwitchV13_0.tunnel_out_port_2nd = 4
		Set Node3_OFSwitchV13_0.OperationMode = OUTBAND
		Set Node3_OFSwitchV13_0.max_queue_length = 100
		Set Node3_OFSwitchV13_0.QueueOutputMode = ROUND_ROBIN
		Set Node3_OFSwitchV13_0.queue_0_min_rate = 0.0
		Set Node3_OFSwitchV13_0.queue_0_max_rate = 100.0
		Set Node3_OFSwitchV13_0.queue_1_min_rate = 0.0
		Set Node3_OFSwitchV13_0.queue_1_max_rate = 100.0
		Set Node3_OFSwitchV13_0.queue_2_min_rate = 0.0
		Set Node3_OFSwitchV13_0.queue_2_max_rate = 100.0
		Set Node3_OFSwitchV13_0.queue_3_min_rate = 0.0
		Set Node3_OFSwitchV13_0.queue_3_max_rate = 100.0
		Set Node3_OFSwitchV13_0.queue_4_min_rate = 0.0
		Set Node3_OFSwitchV13_0.queue_4_max_rate = 100.0
