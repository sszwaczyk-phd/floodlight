			Set Node5_MAC8023_InterfaceID_4.BroInOutLogFile = simple-polska.mac8023_n5_i4_bro_inout.log
			Set Node5_MAC8023_InterfaceID_4.CollLogFile = simple-polska.mac8023_n5_i4_collision.log
