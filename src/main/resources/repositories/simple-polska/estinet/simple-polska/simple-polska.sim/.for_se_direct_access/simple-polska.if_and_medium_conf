			Set Node5_MAC8023_InterfaceID_4.OutThrputLogFile = simple-polska.mac8023_n5_i4_out_thrput.log
			Set Node5_MAC8023_InterfaceID_4.InOutThrputLogFile = simple-polska.mac8023_n5_i4_inout_thrput.log
