		Module Link : Node6_Link_InterfaceID_1
			Set Node6_Link_InterfaceID_1.guitag_remotenode = yes


		Bind Node6_Interface_InterfaceID_1 Node6_MAC8023_InterfaceID_1
		Bind Node6_MAC8023_InterfaceID_1 Node6_PHY_InterfaceID_1
		Bind Node6_PHY_InterfaceID_1 Node6_Link_InterfaceID_1
	EndDefine

