			Set Node4_MAC8023_InterfaceID_5.UniInOutLogFile = simple-polska.mac8023_n4_i5_uni_inout.log
			Set Node4_MAC8023_InterfaceID_5.BroInLogFile = simple-polska.mac8023_n4_i5_bro_in.log
			Set Node4_MAC8023_InterfaceID_5.BroOutLogFile = simple-polska.mac8023_n4_i5_bro_out.log
			Set Node4_MAC8023_InterfaceID_5.BroInOutLogFile = simple-polska.mac8023_n4_i5_bro_inout.log
			Set Node4_MAC8023_InterfaceID_5.CollLogFile = simple-polska.mac8023_n4_i5_collision.log
			Set Node4_MAC8023_InterfaceID_5.DropLogFile = simple-polska.mac8023_n4_i5_drop.log
