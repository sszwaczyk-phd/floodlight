Create Node 8 as Open_vSwitch with name = Open_vSwitch8
	Define InterfaceID 1
		Module Interface : Node8_Interface_InterfaceID_1
			Set Node8_Interface_InterfaceID_1.tunnel_type = tap
			Set Node8_Interface_InterfaceID_1.nat_id = 0
