		Module MAC8023 : Node18_MAC8023_InterfaceID_1
			Set Node18_MAC8023_InterfaceID_1.mac = 00:01:00:00:00:4e
			Set Node18_MAC8023_InterfaceID_1.lock_mac = off
			Set Node18_MAC8023_InterfaceID_1.PromisOpt = off
			Set Node18_MAC8023_InterfaceID_1.mode = full
			Set Node18_MAC8023_InterfaceID_1.log = off
			Set Node18_MAC8023_InterfaceID_1.logInterval = 1
			Set Node18_MAC8023_InterfaceID_1.NumCollision = off
			Set Node18_MAC8023_InterfaceID_1.NumUniInPkt = off
			Set Node18_MAC8023_InterfaceID_1.NumUniOutPkt = off
			Set Node18_MAC8023_InterfaceID_1.NumUniInOutPkt = off
			Set Node18_MAC8023_InterfaceID_1.NumBroInPkt = off
			Set Node18_MAC8023_InterfaceID_1.NumBroOutPkt = off
			Set Node18_MAC8023_InterfaceID_1.NumBroInOutPkt = off
			Set Node18_MAC8023_InterfaceID_1.NumDrop = off
			Set Node18_MAC8023_InterfaceID_1.InThrput = off
			Set Node18_MAC8023_InterfaceID_1.OutThrput = off
			Set Node18_MAC8023_InterfaceID_1.InOutThrput = off
			Set Node18_MAC8023_InterfaceID_1.UniInLogFile = simple-polska.mac8023_n18_i1_uni_in.log
			Set Node18_MAC8023_InterfaceID_1.UniOutLogFile = simple-polska.mac8023_n18_i1_uni_out.log
			Set Node18_MAC8023_InterfaceID_1.UniInOutLogFile = simple-polska.mac8023_n18_i1_uni_inout.log
			Set Node18_MAC8023_InterfaceID_1.BroInLogFile = simple-polska.mac8023_n18_i1_bro_in.log
			Set Node18_MAC8023_InterfaceID_1.BroOutLogFile = simple-polska.mac8023_n18_i1_bro_out.log
			Set Node18_MAC8023_InterfaceID_1.BroInOutLogFile = simple-polska.mac8023_n18_i1_bro_inout.log
			Set Node18_MAC8023_InterfaceID_1.CollLogFile = simple-polska.mac8023_n18_i1_collision.log
			Set Node18_MAC8023_InterfaceID_1.DropLogFile = simple-polska.mac8023_n18_i1_drop.log
			Set Node18_MAC8023_InterfaceID_1.InThrputLogFile = simple-polska.mac8023_n18_i1_in_thrput.log
			Set Node18_MAC8023_InterfaceID_1.OutThrputLogFile = simple-polska.mac8023_n18_i1_out_thrput.log
			Set Node18_MAC8023_InterfaceID_1.InOutThrputLogFile = simple-polska.mac8023_n18_i1_inout_thrput.log
			Set Node18_MAC8023_InterfaceID_1.LinktoHub = 0

		Module PHY : Node18_PHY_InterfaceID_1
			Set Node18_PHY_InterfaceID_1.datarate = 10_Mbps
			Set Node18_PHY_InterfaceID_1.BER = 0.0
			Set Node18_PHY_InterfaceID_1.PropDelay = 1.0
			Set Node18_PHY_InterfaceID_1.linkfail = off
			Set Node18_PHY_InterfaceID_1.linkfailFileName = simple-polska.phy8023_n18_i1.link_failure_duration

		Module Link : Node18_Link_InterfaceID_1
			Set Node18_Link_InterfaceID_1.guitag_remotenode = yes


		Bind Node18_Interface_InterfaceID_1 Node18_MAC8023_InterfaceID_1
		Bind Node18_MAC8023_InterfaceID_1 Node18_PHY_InterfaceID_1
		Bind Node18_PHY_InterfaceID_1 Node18_Link_InterfaceID_1
	EndDefine

EndCreate


