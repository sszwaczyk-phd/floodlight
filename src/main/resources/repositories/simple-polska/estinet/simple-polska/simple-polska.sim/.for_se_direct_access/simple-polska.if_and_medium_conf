Create Node 12 as Open_vSwitch with name = Open_vSwitch12
	Define InterfaceID 1
		Module Interface : Node12_Interface_InterfaceID_1
			Set Node12_Interface_InterfaceID_1.tunnel_type = tap
			Set Node12_Interface_InterfaceID_1.nat_id = 0
